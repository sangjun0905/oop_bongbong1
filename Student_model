#include<vector>
#include "Student.h"

class Student_model{
    private:
        std::vector<Student> student_list;

    public:

        void add_student(const Student& new_student){
            student_list.push_back(new_student);
        } //학생 추가

        const std::vector<Student>& get_all_students(){
            return student_list;
        }//전체 리스트 반환

    };